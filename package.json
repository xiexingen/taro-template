    "@tarojs/plugin-sass": "1.2.25",
    "@tarojs/plugin-uglifyjs": "1.2.25",
    "@tarojs/webpack-runner": "1.2.25",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^8.2.3",
