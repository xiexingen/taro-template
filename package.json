    "@tarojs/taro-alipay": "1.2.25",
    "@tarojs/taro-h5": "1.2.25",
    "@tarojs/taro-swan": "1.2.25",
    "@tarojs/taro-tt": "1.2.25",
    "@tarojs/taro-weapp": "1.2.25",
    "dva-core": "^1.4.0",
    "dva-immer": "^0.4.2",
    "dva-loading": "^3.0.2",
    "dva-model-extend": "^0.1.2",
    "nerv-devtools": "^1.3.9",
    "nervjs": "^1.3.9"
