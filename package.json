{
	"name": "taro-template",
	"version": "1.0.0",
	"private": true,
	"description": "基于taro+dva+ts的模板项目",
	"scripts": {
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"test": "umi-test",
		"lint": "umi-lint  --tslint --eslint --stylelint ./src",
		"precommit": "umi-lint --staged --tslint  --eslint --stylelint --prettier --fix"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@tarojs/components": "1.3.8",
		"@tarojs/redux": "1.3.8",
		"@tarojs/router": "1.3.8",
		"@tarojs/taro": "1.3.8",
		"@tarojs/taro-alipay": "1.3.8",
		"@tarojs/taro-h5": "1.3.8",
		"@tarojs/taro-swan": "1.3.8",
		"@tarojs/taro-tt": "1.3.8",
		"@tarojs/taro-weapp": "1.3.8",
		"dva-core": "^1.4.0",
		"dva-immer": "^0.4.11",
		"dva-loading": "^3.0.12",
		"dva-model-extend": "^0.1.2",
		"nerv-devtools": "^1.4.0",
		"nervjs": "^1.4.0",
		"taro-ui": "^2.2.1"
	},
	"devDependencies": {
		"@tarojs/plugin-babel": "1.3.8",
		"@tarojs/plugin-csso": "1.3.8",
		"@tarojs/plugin-sass": "1.3.8",
		"@tarojs/plugin-uglifyjs": "1.3.8",
		"@tarojs/webpack-runner": "1.3.8",
		"@types/react": "^16.8.19",
		"@types/webpack-env": "^1.13.9",
		"@typescript-eslint/parser": "^1.10.2",
		"babel-eslint": "^10.0.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"eslint": "^5.16.0",
		"eslint-config-taro": "1.3.8",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-react": "^7.13.0",
		"eslint-plugin-taro": "1.3.8",
		"stylelint-config-css-modules": "^1.4.0",
		"stylelint-config-prettier": "^5.2.0",
		"stylelint-config-standard": "^18.3.0",
		"stylelint-declaration-block-no-ignored-properties": "^2.1.0",
		"tslint": "^5.18.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-react": "^4.0.0",
		"typescript": "^3.5.3",
		"umi-lint": "^2.0.2"
	}
